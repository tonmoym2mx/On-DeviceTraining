<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.tonmoy.on_devicetraining.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_labelDetailsFragment"
            app:destination="@id/labelDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/labelDetailsFragment"
        android:name="com.tonmoy.on_devicetraining.ui.label_details.LabelDetailsFragment"
        android:label="fragment_label_details"
        tools:layout="@layout/fragment_label_details" />

</navigation>